{
  "taskRoleArn": "arn:aws:iam::774076615373:role/ECSServiceRoleRSF",
  "containerDefinitions": [
    {
      "name": "item_indexer",
      "image": "774076615373.dkr.ecr.us-east-1.amazonaws.com/bbb-labs/item_indexer:latest",
      "portMappings": [
        {
          "protocol" : "tcp",
          "containerPort" : 3000,
          "hostPort" : 0
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "upc",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "stag"
        }
      },
      "cpu": 500,
      "memory": 500,
      "essential": true,
      "mountPoints": [],
      "volumesFrom": [],
      "environment" : [
        { "name" : "ENVIRONMENT_TOKEN", "value" : "upc-stag1" },
        { "name" : "SECRETS_BUCKET_NAME", "value" : "bbb-secrets" },
        { "name" : "RAILS_LOG_TO_STDOUT", "value" : "true" }
      ]
    }
  ],
  "family": "item_indexer",
  "networkMode": "bridge",
  "placementConstraints": []
}
